<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://www.hibernate.org/dtd/hibernate-mapping"> 
 
<hibernate-mapping>
 
    <class name="logic.Person" table="person">
 
        <id name="id" column="id">
            <generator class="native" />
        </id>
 
        <property name="first_name">
            <column name="first_name" length="16" not-null="true" />
        </property>
 
        <property name="last_name">
            <column name="last_name" length="16" not-null="true" />
        </property>
 
        <property name="age">
            <column name="age" length="16" not-null="true" />
        </property>
 
        <property name="street_id">
            <column name="street_id" length="16" not-null="true" />
        </property>
 
    </class>
</hibernate-mapping>